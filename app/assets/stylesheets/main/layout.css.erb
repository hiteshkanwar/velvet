.click-nav ul 
{
	font-weight:900;
}

.click-nav ul li 
{
	position:relative;
	list-style:none;
	cursor:pointer;
}

.click-nav ul .clicker 
{
	color:#fff;
	text-transform:uppercase;
	padding:8px 35px 8px 10px;
	background:#bc0000;
	cursor:pointer;
	border:none;
	background-image:url(<%=asset_path "main/direct-button.jpg"%>);
	background-position:right 11px;
	background-repeat:no-repeat;
