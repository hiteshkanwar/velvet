.border-radius
{
	border-radius:3px;
	-webkit-border-radius:3px;
	-moz-border-radius:3px;
	-ms-border-radius:3px;
	-o-border-radius:3px;
	-khtml-border-radius:3px;
}

a.button, .button
{
	color:#fff;
	margin:0 auto;
	font-size:12px;
	font-weight:bold;
	text-transform:capitalize;
	padding:5px 25px 5px 10px;
	background:#bc0000;
	cursor:pointer;
	background-image:url(<%=asset_path "main/button-arrow.png"%>);
	background-position:right 9px;
	background-repeat:no-repeat;
	border-radius:3px;
	-webkit-border-radius:3px;
	-moz-border-radius:3px;
	-ms-border-radius:3px;
	-o-border-radius:3px;
	-khtml-border-radius:3px;
}

a.button:hover, .button:hover
{
	background:#333;
	background-image:url(<%=asset_path "main/button-arrow.png"%>);
	background-position:right 9px;
	background-repeat:no-repeat;
}

.black
{
	padding:5px 10px;
