.wrapper .page .container .right-section .profile .label-holder p.check
{
	margin:5px 0 0 0;
}

.wrapper .page .container .right-section .profile .label-holder .profile-photo
{
	width:80px;
	height:60px;
	float:left;
	background:#333;
}

.wrapper .page .container .right-section .profile .label-holder .profile-photo img
{
	border-radius:3px;
	-webkit-border-radius:3px;
	-moz-border-radius:3px;
	-ms-border-radius:3px;
	-o-border-radius:3px;
	-khtml-border-radius:3px;
}

.wrapper .page .container .right-section .profile .label-holder .change-photo
{
	width:360px;
	float:left;
	margin:0 0 0 10px;
}

.wrapper .page .container .right-section .profile .label-holder .change-photo .button
{
	border:none;
	color:#fff;
	width:auto;
	text-align:left;
	padding:10px 20px;
	margin:0 0 10px 0;
	background-image:none;
}

.wrapper .page .container .right-section .profile .label-holder .change-photo span
{
	font-size:13px;
	color:#333;
	line-height:20px;
	padding:0;
	display:block;
}

.wrapper .page .container .right-section .profile .label-holder .button
{
	border:none;
	color:#fff;
	width:160px;
	text-align:left;
	padding:10px 0 10px 10px;
	background-position:right 12px;
}

.wrapper .page .container .right-section .profile .label-holder .button-inner
{
	border:none;
	color:#fff;
	width:auto;
	text-align:left;
	padding:10px;
	margin:10px 0 0 0;
	background-image:none;
}

.wrapper .page .container .right-section .profile .save
{
	border:none;
	color:#fff;
	width:110px;
	margin:20px 0 0 0;
	text-align:center;
	padding:10px 0;
	background-image:none;
	float:right;
}


.wrapper .page .container .right-section .profile .label-holder p span
{
	font-size:12px;
	padding:0;
	display:block;
	line-height:18px;
}

.wrapper .page .container .right-section .profile hr
{
	border-top:1px solid #f0f0f0;
	margin:15px 0;
}

.wrapper .page .container .right-section .profile .profile-heading
{
	color:#333;
	font-size:18px;
	font-weight:600;
	text-transform:capitalize;
}

.wrapper .footer-bootom
{
	background:url(<%=asset_path "main/header.png"%>) top center no-repeat #5e0000;
	padding:20px 0;
	margin-top:20px;
	color:#fff;
}

.wrapper .footer-bootom ul
{
	margin:20px 0 15px 0;
}

.wrapper .footer-bootom ul li
{
	display:inline;
	padding:0 5px;
}

.wrapper .footer-bootom ul li a
{
	color:#fff;
}






